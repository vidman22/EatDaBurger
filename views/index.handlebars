<div class="container">
  <div id="title">
      <h1>Eat-Da-Burger</h1>
  </div>
</div>

<br><br><br>

<hr>

<div class="container">


<div class= "row">

    <div class="col-md-6">


      <h2>Uneaten Burgers</h2>
      <ul>
        {{#each burgers}}
          {{#unless this.devoured}}
              <li>
              {{this.burger_name}}

              <form action="burgers/update/{{this.id}}?_method=PUT" method="POST">
                <input type="hidden" name="devoured" value="1">
                <button type="submit">Eat!</button>
              </form>
            </li> 
          {{/unless}}
        {{/each}}
      </ul>

    </div>

    <div class="col-md-1">
    </div>

    <div class="col-md-5">


      <h2>Eaten Burgers</h2>
      <ul>
        {{#each burgers}}
          {{#if this.devoured}}
              <li>
              {{this.burgers_name}}
              <form action="burgers/update{{this.id}}?_method=PUT" method="POST">
                <input type="hidden" name="devoured" value="0">
                <button type="submit" id="eatButton"></button>
              </form>
              <form action="burgers/delete/{{this.id}}?_method=DELETE" method="POST">
                <button type="submit" id="eatButton"></button>
              </form>
              </li>
          {{/if}}
        {{/each}}
      </ul>
    </div>
  </div>
</div>

<div class="container">
    <h2>Add a Burger</h2>
    <form class="create-form" action="burgers/create" method="POST">
      <input name="burgerName" type="text" placeholder="Burger Name" class="form-control">
      

      
      <button type="submit">Add Burger</button>
    </form>

  <br><br>
</div>
